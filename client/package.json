{
  "name": "no-garbage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env MODE=DEVELOPMENT next dev -H localhost -p 4141",
    "build": "cross-env MODE=DEVELOPMENT next build",
    "start": "cross-env MODE=DEVELOPMENT next start -H localhost -p 4141",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true npm run build",
    "start:pm2:dev": "cross-env MODE=DEVELOPMENT next build && pm2 startOrRestart next.config.js --name no-garbage -p 4141",
    "start:pm2:prod": "cross-env MODE=PRODUCTION pm2 start npm --name no-garbage -- run start -- -p 4141",
    "start:forever": "forever start npm -- start",
    "stopt:pm2": "pm2 stop no-garbage && pm2 delete no-garbage"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-language": "^1.0.1",
    "@next-auth/firebase-adapter": "2.0.1",
    "@next/bundle-analyzer": "^13.2.4",
    "antd": "^5.7.0",
    "axios": "^1.3.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "eslint-config-next": "13.1.1",
    "firebase": "^9.15.0",
    "firebase-admin": "^11.9.0",
    "i18next": "^22.4.7",
    "mapbox-gl": "^2.12.0",
    "next": "13.4.12",
    "next-auth": "^4.22.1",
    "next-i18next": "^13.0.2",
    "nextjs-progressbar": "^0.0.16",
    "pm2": "^5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-i18next": "^12.1.1",
    "react-redux": "^8.0.5",
    "sass": "^1.61.0",
    "sharp": "^0.32.0",
    "swiper": "^9.1.1",
    "uninstall": "^0.0.0",
    "uuid": "^9.0.0"
  },
  "compilerOptions": {
    "baseUrl": "src"
  },
  "devDependencies": {
    "@next/font": "^13.1.2",
    "@reduxjs/toolkit": "^1.9.1",
    "@svgr/webpack": "^7.0.0",
    "@types/mapbox-gl": "^2.7.10",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "8.30.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "styled-components": "^5.3.9",
    "typescript": "5.0.4"
  }
}
